<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <servlet>
        <servlet-name>ManterDiretorioController</servlet-name>
        <servlet-class>controller.ManterDiretorioController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ManterBuildController</servlet-name>
        <servlet-class>controller.ManterBuildController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ManterLerBuildController</servlet-name>
        <servlet-class>controller.ManterLerBuildController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ManterDiretorioEnvironmentController</servlet-name>
        <servlet-class>controller.ManterDiretorioEnvironmentController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ManterDiretorioController</servlet-name>
        <url-pattern>/ManterDiretorioController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ManterBuildController</servlet-name>
        <url-pattern>/ManterBuildController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ManterLerBuildController</servlet-name>
        <url-pattern>/ManterLerBuildController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ManterDiretorioEnvironmentController</servlet-name>
        <url-pattern>/ManterDiretorioEnvironmentController</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
